[difftool "ediff"]
        cmd = ~/dotfiles/cmd/ediff $LOCAL $REMOTE $MERGED

[merge]
        tool = ediff

[mergetool "ediff"]
        cmd = ~/dotfiles/cmd/ediff $LOCAL $REMOTE $MERGED $BASE
        trustExitCode = true

[core]
        editor = emacsclient
	excludesfile = ~/.gitignore_global
[diff]
	external = /Users/tomasherman/dotfiles/cmd/opendiff-git.sh
